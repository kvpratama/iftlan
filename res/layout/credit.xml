<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ImageView android:src="@drawable/logo" android:layout_width="320dp"
		android:layout_height="112dp" android:id="@+id/imageView1"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_marginTop="53dp"></ImageView>
	<ImageButton android:src="@drawable/back" android:id="@+id/back"
		android:layout_width="wrap_content" android:layout_height="40dp"
		android:layout_below="@+id/imageView2"
		android:layout_centerHorizontal="true" android:layout_marginTop="25dp"></ImageButton>
	<ImageView android:src="@drawable/createdby"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:id="@+id/imageView2" android:layout_below="@+id/imageView1"
		android:layout_centerHorizontal="true" android:layout_marginTop="35dp"></ImageView>
</RelativeLayout>
